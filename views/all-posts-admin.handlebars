{{!-- js related to it--}}
<div>
    {{#each posts }}
        <div class="card">
            <div class="card-header">
                <h2>
                   <a href="/dashboard/edit/{{id}}">{{title}}</a>
                </h2>
                <div>
                    posted by {{User.username}} on {{format_date createdAt}} 
                </div>
            </div>
            <div class="card-body">{{body}}</div>
        </div>
    {{/each}}
</div>